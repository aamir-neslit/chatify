{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": [
      "es2017"
    ],
    "allowJs": true,
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@components": [
        "components"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@navigation": [
        "navigation"
      ],
      "@screens": [
        "screens"
      ],
      "@services": [
        "services"
      ],
      "@constants": [
        "constants"
      ],
      "@assets": [
        "assets"
      ]
    },
    "typeRoots": [],
    "types": [],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
